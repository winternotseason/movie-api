{
    "version": 2,
    "builds": [
      {
        "src": "app.js",			
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/app.js"
      }
    ],
    "headers" : [
      {
        "source" : "/(.*)",
        "headers" : [
          {
            "key": "Access-Control-Allow-Origin", "value": "*"
          } 
        ]
      }
    ] 
  }